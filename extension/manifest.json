{
  "manifest_version": 3,
  "name": "Useapps",
  "version": "1.0.12",
  "description": "Useapps extention",
  "key": "",
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "cookies",
    "tabs",
    "idle",
    "alarms",
    "scripting",
    "offscreen",
    "browsingData",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],

  "host_permissions": [
    "<all_urls>"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    },
      {
    "matches": ["https://www.upwork.com/*"],
    "js": ["upwork-content.js"],
    "run_at": "document_start"
    },
    {
    "matches": ["<all_urls>"],
    "js": ["autofill-content.js"],
    "run_at": "document_idle"
    }
  ],

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "oauth2": {
    "client_id": "126571944175-ljttvgip890m5u5876j7ocd1qc3ted44.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}